# Configuración de Firebase
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Configuración de Stripe
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
VITE_STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key

# Configuración de Google Analytics
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# URL base de la aplicación
VITE_BASE_URL=https://av10dejulio.cl

# Configuración de optimización de imágenes
VITE_IMAGE_OPTIMIZATION_URL=https://res.cloudinary.com/your_account/image/fetch

# Configuración de monitoreo
VITE_MONITORING_ENABLED=true
VITE_LOG_LEVEL=info

# Configuración de backup
VITE_BACKUP_ENABLED=true
VITE_BACKUP_INTERVAL=86400000

# Configuración de desarrollo
NODE_ENV=development
VITE_DEBUG=true
